    "@babel/runtime": "^7.5.5",
    "express": "^4.17.1",
    "pg": "^7.12.1",
    "sequelize": "^5.14.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "concurrently": "^4.1.1",
    "nodemon": "^1.19.1",
    "sequelize-cli": "^5.5.0"
  }
